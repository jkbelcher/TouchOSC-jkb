<key><![CDATA[script]]></key>
<value><![CDATA[local handled = false

function onValueChanged(key)
  if key == "touch" then
    if self.values.touch == true then  
      -- rise/press of touch
      if handled then
        -- ignore drag
        return
      end
      handled = true
      self.parent:notify("toggle")
    else
      -- fall/release of touch
      handled = false
    end
  end
end]]></value>
